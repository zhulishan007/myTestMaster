@model Mars_Data.Models.T_TESTER_INFO

@{
    ViewBag.Title = "AddUser";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="kt-portlet">
            <div class="kt-portlet__head">
                <div class="kt-portlet__head-label">
                    <h3>
                        Create User
                    </h3>
                </div>
            </div>
            <!--begin::Form-->
            <form class="kt-form kt-form--label-right" id="kt_form_1">
                <div class="kt-portlet__body">
                    <div class="form-group form-group-last kt-hide">
                        <div class="alert alert-danger" role="alert" id="kt_form_1_msg">
                            <div class="alert-icon"><i class="flaticon-warning"></i></div>
                            <div class="alert-text">
                                Oh snap! Change a few things up and try submitting again.
                            </div>
                            <div class="alert-close">
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                    <span aria-hidden="true"><i class="la la-close"></i></span>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="form-group row" hidden="hidden">
                        <label class="col-form-label col-lg-3 col-sm-12">UserID</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="Userid" hidden="hidden">

                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">First Name</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="Firstname">

                        </div>
                    </div>


                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">Middle Name</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="Middlename">

                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">Last Name</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="LastName">

                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">Email Address</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="EmailId">

                        </div>
                    </div>


                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">User Name</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="Username">

                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">Password</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email" id="Password">

                        </div>
                    </div>

                    <div class="form-group row">
                        <label class="col-form-label col-lg-3 col-sm-12">Confirm Password</label>
                        <div class="col-lg-9 col-md-9 col-sm-12">
                            <input type="text" class="form-control" name="email">

                        </div>
                    </div>


                </div>
                <div class="kt-portlet__foot">
                    <div class="kt-form__actions">
                        <div class="row">
                            <div class="col-lg-9 ml-lg-auto">
                                <button type="submit" class="btn btn-outline-brand btn-elevate btn-pill" id="btnAdd">Create</button>
                                <button type="reset" class="btn btn-outline-brand btn-elevate btn-pill">Cancel</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <!--end::Form-->
        </div>

    </div>
</div>
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script type="text/javascript">
    function loaddata() {
        debugger;
        $.ajax({
            type: 'GET',
            dataType: "json",
            url: "http://localhost/MARWebAPI/api/Listusers",
            success: function (result) {
                if (result && result.length > 0) {
                    for (var i = 0; i < result.length; i++) {
                        $("#kt_table").append("<tr><td>" + result[i].TESTER_NAME_LAST + "</td><td>" + result[i].TESTER_NAME_M + "</td><td>" + result[i].TESTER_NAME_F + "</td><td>" + result[i].TESTER_LOGIN_NAME + "</td><td>" + result[i].TESTER_MAIL + "</td></tr>");
                    }
                }
            }
        });
    }
    $(document).ready(function () {
        $("#btnAdd").click(function () {
            debugger;
            var userObj = {
                UserID=$("Userid").val(),
                FirstName=$("Firstname").val(),
                MiddleName=$("Middlename").val(),
                LastName=$("LastName").val(),
                Emailid=$("Userid").val(),
                Username=$("Userid").val(),
                Password=$("Userid").val(),

            };
           
    });
    });
    //function Add() {
    //    debugger;
    //    var userObj = {
    //        TESTER_ID:
    //    };
    //}
</script>


