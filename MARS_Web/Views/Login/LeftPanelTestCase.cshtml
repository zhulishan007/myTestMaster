@using MARS_Repository.ViewModel
@model List<TestCaseListByProject>

@if (Model != null)
{
    foreach (TestCaseListByProject item in Model)
    {
        <span class="kt-menu__arrow"></span>
        <ul class="kt-menu__subnav">
            <li class="kt-menu__item lsdataset context-menu-TestCase " aria-haspopup="true">
                <input type="hidden" id="projectidtc" />
                <input type="hidden" id="testsuiteidtc" />
                <a href="javascript:;" class="kt-menu__link kt-menu__toggle context-TestCase" data-tab="TestCase" title="@item.TestCaseDesc" data-name="@item.TestcaseName" data-id="@item.TestcaseId" data-Project-id="@item.ProjectId" data-TestSuite-id="@item.TestsuiteId">
                    @*onclick="PartialRightGrid(@item.TestcaseId,@item.TestsuiteId,@item.ProjectId,'@item.TestcaseName',null,$(this));
                        GetDataSet(@item.ProjectId,@item.TestsuiteId,@item.TestcaseId,'@item.ProjectName','@item.TestsuiteName','@item.TestcaseName', $(this)); ">*@

                    <span class="kt-menu__link-icon" data-tab="TestCase" title="@item.TestCaseDesc" data-name="@item.TestcaseName" data-id="@item.TestcaseId" data-Project-id="@item.ProjectId" data-TestSuite-id="@item.TestsuiteId" onclick="GetDataSet(@item.ProjectId,@item.TestsuiteId,@item.TestcaseId,'@item.ProjectName','@item.TestsuiteName','@item.TestcaseName', $(this));">
                        <i class="fa fa-folder-plus"></i>
                    </span>
                    <span class="kt-menu__link-text" data-tab="TestCase" title="@item.TestCaseDesc" data-name="@item.TestcaseName" data-id="@item.TestcaseId" data-Project-id="@item.ProjectId" data-TestSuite-id="@item.TestsuiteId" onclick="PartialRightGrid(@item.TestcaseId,@item.TestsuiteId,@item.ProjectId,'@item.TestcaseName',null,$(this),'0');">@item.TestcaseName <span class="tccount"> (@item.DataSetCount)</span></span>
                </a>
                <div class="kt-menu__submenu "></div>
            </li>
        </ul>
    }
}
