<script>
    Highcharts.chart('chartholder', {

        title: {
            text: ''
        },

        subtitle: {
            text: ''
        },

        yAxis: {
            title: {
                text: ''
            }
        },

        xAxis: {
            categories: xAxisData(),
     },

        legend: {
            layout: 'vertical',
            align: 'right',
            verticalAlign: 'middle'
        },

        @*plotOptions: {
            series: {
                label: {
                    connectorAllowed: false
                },
                pointStart: @ViewBag.X_Start,
            }
        },*@

        series: 
            getData(),

        responsive: {
            rules: [{
                condition: {
                    maxWidth: 500
                },
                chartOptions: {
                    legend: {
                        layout: 'horizontal',
                        align: 'center',
                        verticalAlign: 'bottom'
                    }
                }
            }]
        }

    });

    function getData() {
         
           var json_axis = @Html.Raw(Json.Encode(@ViewBag.axis));
            console.log(json_axis);
            return JSON.parse(json_axis);
    }
        
function xAxisData() {

           var json_axis = @Html.Raw(Json.Encode(@ViewBag.X_Start));
        console.log(json_axis);
        return JSON.parse(json_axis);
        }
</script>