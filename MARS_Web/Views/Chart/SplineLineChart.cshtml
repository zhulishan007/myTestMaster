<script>
    Highcharts.chart('chartholder', {
        chart: {
            type: 'spline'
        },
        title: {
            text: ''
        },
        subtitle: {
            text: ''
        },
        xAxis: {
            categories: xAxisData(),
            title: {
                text: ''
            }
        },
        yAxis: {
            title: {
                text: ''
            },
          
        },
        //tooltip: {
        //    headerFormat: '<b>{series.name}</b><br>',
        //    pointFormat: '{point.x:%e. %b}: {point.y:.2f} m'
        //},

        plotOptions: {
            series: {
                marker: {
                    enabled: true
                }
            }
        },

        colors: ['#6CF', '#39F', '#06C', '#036', '#000'],

       
        series:  getData(),

        responsive: {
            rules: [{
                condition: {
                    maxWidth: 500
                },
                chartOptions: {
                    plotOptions: {
                        series: {
                            marker: {
                                radius: 2.5
                            }
                        }
                    }
                }
            }]
        }
    });

     function getData() {
         
           var json_axis = @Html.Raw(Json.Encode(@ViewBag.axis));
            console.log(json_axis);
         return JSON.parse(json_axis);
    }
    function xAxisData() {

           var json_axis = @Html.Raw(Json.Encode(@ViewBag.X_Start));
        console.log(json_axis);
        return JSON.parse(json_axis);
        }
</script>