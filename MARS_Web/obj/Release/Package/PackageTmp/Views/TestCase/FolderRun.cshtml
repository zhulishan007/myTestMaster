<div class="kt-portlet__head kt-portlet__head--lg">
    <div class="kt-portlet__head-label">
        <h3 class="kt-portlet__head-title" style="color:#1582ab">
            Download Folder Files
        </h3>
    </div>
</div>
<div class="mt-4">
    @{
        if (!string.IsNullOrEmpty(ViewBag.reportfile))
        {
            <div class="form-group col-md-4" id="hdnDownloadReportlink">
                <div></div>
                <div class="custom-file ">
                    <a href="/TestSuite/DownloadFile?path=@ViewBag.reportfile" id="downloadTestSuiteFile"> Click here to download Report File <i class="flaticon-download-1"></i>  </a>
                </div>
            </div>
        }
    }

    @{
        if (!string.IsNullOrEmpty(ViewBag.logfile))
        {
            <div class="form-group col-md-4" id="hdnDownloadLoglink">
                <div></div>
                <div class="custom-file ">
                    <a href="/TestSuite/DownloadFile?path=@ViewBag.logfile" id="downloadTestSuiteFile"> Click here to download Log File <i class="flaticon-download-1"></i>  </a>
                </div>
            </div>
        }
    }

</div>
